{
  "name": "csim",
  "private": true,
  "description": "A simple scouting system",
  "version": "0.1.0",
  "author": "Thanawat Techaumnuaiwit, Berniece Lau, Serena Chen. @Team4159",
  "dependencies": {
    "@emotion/core": "^10.0.7",
    "@emotion/styled": "^10.0.7",
    "@types/ramda": "^0.25.48",
    "@types/victory": "^31.0.8",
    "axios": "^0.18.0",
    "emotion-theming": "^10.0.7",
    "eslint-config-prettier": "^3.6.0",
    "formik": "^1.5.0",
    "gatsby": "^2.0.118",
    "gatsby-cli": "^2.4.9",
    "gatsby-image": "^2.0.29",
    "gatsby-plugin-emotion": "^4.0.3",
    "gatsby-plugin-layout": "^1.0.12",
    "gatsby-plugin-manifest": "^2.0.17",
    "gatsby-plugin-offline": "^2.0.23",
    "gatsby-plugin-react-helmet": "^3.0.6",
    "gatsby-plugin-sharp": "^2.0.20",
    "gatsby-plugin-typescript": "^2.0.7",
    "gatsby-plugin-typography": "^2.2.7",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.4",
    "gatsby-source-filesystem": "^2.0.20",
    "gatsby-transformer-sharp": "^2.1.13",
    "prop-types": "^15.7.1",
    "ramda": "^0.26.1",
    "react": "^16.8.1",
    "react-dom": "^16.8.1",
    "react-helmet": "^5.2.0",
    "react-pose": "^4.0.6",
    "react-table": "6.9.0",
    "react-typography": "^0.16.18",
    "simple-statistics": "^7.0.1",
    "typography": "^0.16.18",
    "typography-theme-kirkham": "^0.16.18",
    "typography-theme-ocean-beach": "^0.16.18",
    "victory": "^31.2.0",
    "yup": "^0.26.10"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build --prefix-paths",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "format": "prettier-eslint --write 'src/**/*.ts*'",
    "lint": "eslint './**/*.ts*'",
    "test": "jest",
    "type-check": "tsc --noEmit --JSX react",
    "lint-staged": "lint-staged",
    "type-check:watch": "npm run type-check -- --watch",
    "graphql-types": "gql-gen --url http://localhost:8000/___graphql --template typescript --out ./src/graphql-types.d.ts"
  },
  "devDependencies": {
    "@types/jest": "^23.3.14",
    "@types/react-helmet": "^5.0.8",
    "@types/react-table": "^6.7.21",
    "@types/typography": "^0.16.2",
    "@types/yup": "^0.26.9",
    "@typescript-eslint/eslint-plugin": "^1.3.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.13.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-standard": "^4.0.0",
    "graphql-code-generator": "^0.15.2",
    "graphql-codegen-typescript-template": "^0.15.2",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "jest-dom": "^3.1.1",
    "lint-staged": "^8.1.3",
    "prettier": "^1.16.4",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^4.7.1",
    "react-testing-library": "^5.6.0",
    "ts-jest": "^23.10.1",
    "ts-node": "^8.0.2",
    "typescript": "^3.3.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier-eslint --write"
    ],
    "*.{js,json,md}": [
      "prettier-eslint --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(tsx?|jsx?)$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "typeface-*": "identity-obj-proxy",
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file.js"
    },
    "collectCoverage": false,
    "coverageReporters": [
      "lcov",
      "text",
      "html"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
